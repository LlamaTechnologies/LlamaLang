    "cmake.configureOnOpen": true,
    "cmake.buildDirectory": "${workspaceFolder}/project",
    "cmake.cmakePath": "/usr/bin/cmake",
    "cmake.configureSettings": {
        "LLVM_DIR": "/usr/lib/cmake/llvm"
    },
    "cmake.debugConfig": {
        "args": [
            "-s",
